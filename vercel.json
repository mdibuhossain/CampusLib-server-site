{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@now/node"
        }
    ],
    "crons": [
        {
            "path": "./",
            "schedule": "0 10 * * *"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}